{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "lib": ["es2017"],
    "skipLibCheck": true,
    "paths": {
      "#/*": ["src/*"]
    },
  },
  "include": ["src/**/*"],
  "allowJs": true,
  "target": "es2017"
}
